# Troud Environment Variables Example
# Copy this file to .env and fill with your values

# API Configuration
VITE_API_BASE_URL=https://your-api-domain.com/api
VITE_IMAGE_UPLOAD_URL=https://your-api-domain.com/api/images

# External Services  
VITE_WHATSAPP_API_URL=https://api.whatsapp.com/send

# CDN/Storage
VITE_S3_BASE_URL=https://your-bucket.s3.amazonaws.com

# App Configuration
VITE_APP_NAME=Troud
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=production
VITE_DEBUG=false
